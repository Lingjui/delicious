<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- Generated by MyEclipse Hibernate Tools. -->
<!-- package声明类所在包，如果不写那么在class和name属性里需要指定包名全路径 -->
<hibernate-mapping>
<!-- class指映射一下user类，提供了公共的无参构造方法，通过反射产生对象，
属性用private修饰，并且生成get，set，不能用final修饰 -->
	<class name="com.qst.model.User" table="user">
	<!-- ID表示实体类的标识，对应数据库表中的主键
	native：根据底层数据库能力选择identity，sequence，或者Hilo中的一个 -->
		<id name="id" column="id" type="int">
			<generator class="native" /><!--生产者发生器 -->
		</id>
		<property name="username" column="username" type="string" />
		<property name="password" column="Password" type="string"  />
		<property name="email" column="email" type="string" />
		<property name="sex" column="sex" type="int" />
		<property name="birthday" column="birthday" type="string"  />
		<property name="job" column="job" type="string" />
		<property name="sign" column="sign" type="string" /> 
		<property name="photo" column="photo" type="string" />
		<property column="date" name="date" type="date"/> 
		<!-- <many-to-one name="user" class="com.qst.model.User" column="user_id"/> -->
	</class>

</hibernate-mapping>